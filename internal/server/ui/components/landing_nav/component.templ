	<div id="mobile-menu" class="mobile-menu" onclick="if(event.target === this) this.classList.remove('mobile-menu--open')">
		<div class="mobile-menu-content">
			<div class="mobile-menu-header">
				<span class="mobile-menu-title">Menu</span>
				<button class="mobile-menu-close" onclick="document.getElementById('mobile-menu').classList.remove('mobile-menu--open')" aria-label="Close menu">
					<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
						<line x1="18" y1="6" x2="6" y2="18"></line>
						<line x1="6" y1="6" x2="18" y2="18"></line>
					</svg>
				</button>
			</div>
			<nav class="mobile-menu-nav">
				<a href="/" class="mobile-menu-link">
